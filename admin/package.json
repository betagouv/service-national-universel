    "preview": "vite preview",
    "postinstall": "npm run copy:dsfr",
    "predev": "only-include-used-icons",
    "copy:dsfr": "copy-dsfr-to-public && only-include-used-icons",
    "lint": "eslint src --ext .js,.jsx",

    "coverage": "jest --runInBand --coverage --silent",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js --fix",
    "lint:report": "eslint src --ext .js --format @microsoft/eslint-formatter-sarif --output-file eslint-results.sarif",

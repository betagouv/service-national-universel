{
  "LABEL": {
    "name": "Pull request title validation",
    "color": "EEEEEE"
  },
  "CHECKS": {
    "regexp": "^(chore\\(|feat\\(|fix\\()(release,? ?|app,? ?|admin,? ?|misc,? ?|api,? ?)+\\) ?:.*$",
    "regexpFlags": "i",
    "ignoreLabels" : ["dont-check-PRs-with-this-label", "meta"]
  },
  "MESSAGES": {
    "success": "All OK",
    "failure": "Failing CI test",
    "notice": ""
  }
}


{
  "LABEL": {
    "name": "Pull request title validation",
    "color": "EEEEEE"
  },
  "CHECKS": {
    "regexp": "^(chore\\(|feat\\(|refactor\\(|fix\\()(release,? ?|app,? ?|admin,? ?|misc,? ?|api,? ?|lib,? ?)+\\) ?:.*$",
    "regexpFlags": "i",
    "ignoreLabels": [
      "bypass title",
      "meta"
    ]
  },
  "MESSAGES": {
    "success": "Title is compliant",
    "failure": "Title is not respecting the guidelines.",
    "notice": ""
  }
}
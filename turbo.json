{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [
        ".dist/**",
        "!.dist/cache/**"
      ]
    },
    "start": {
      "outputs": [
        ".launch/**",
        "!.launch/cache/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "test": {
      "cache": false,
      "persistent": true,
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "lint": {}
  }
}